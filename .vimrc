set title
syntax on

set tabstop=4
set smartindent
set autoindent

"set number
set nocompatible
set whichwrap=b,s,h,l,<,>,[,]
set showmatch
set showmode
set encoding=utf-8 nobomb
set ruler
set showcmd
set shiftwidth=4
set expandtab
set backupskip=/tmp/*,/private/tmp/*
set backspace=indent,eol,start
colorscheme elflord

set ignorecase
set smartcase
set wrapscan
